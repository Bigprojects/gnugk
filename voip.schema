## 1.3.6.1.4.1.9564.1.5   = VoIP object classes
#  1.3.6.1.4.1.9564.2     = attributes
#  1.3.6.1.4.1.9564.2.1   = mediaWays attributes
#  1.3.6.1.4.1.9564.2.1.1 = mediaWays online-services attributes
#  1.3.6.1.4.1.9564.2.1.2 = mediaWays mail attributes
#  1.3.6.1.4.1.9564.2.1.3 = mediaWays Unified Messaging attr.
#  1.3.6.1.4.1.9564.2.2   = Netscape attributes
#  1.3.6.1.4.1.9564.2.3   = DHCP attributes
#  1.3.6.1.4.1.9564.2.4   = RWE attributes
## 1.3.6.1.4.1.9564.2.5   = mediaWays VoIP attributes
#
# SYNTAX overview
# binary                     1.3.6.1.4.1.1466.115.121.1.5 BER/DER data 
# boolean		     1.3.6.1.4.1.1466.115.121.1.7 boolean value 
# distinguishedName          1.3.6.1.4.1.1466.115.121.1.12 DN 
# directoryString	     1.3.6.1.4.1.1466.115.121.1.15 UTF-8 string 
# Facsimile Telephone Number 1.3.6.1.4.1.1466.115.121.1.22
# IA5String		     1.3.6.1.4.1.1466.115.121.1.26 ASCII string 
# Integer	             1.3.6.1.4.1.1466.115.121.1.27 integer 
# Name and Optional UID      1.3.6.1.4.1.1466.115.121.1.34 DN plus UID 
# Numeric String	     1.3.6.1.4.1.1466.115.121.1.36 numeric string 
# OID			     1.3.6.1.4.1.1466.115.121.1.38 object identifier 
# Octet String		     1.3.6.1.4.1.1466.115.121.1.40 arbitary octets 
# Printable String           1.3.6.1.4.1.1466.115.121.1.44 printable string
# Telephone Number           1.3.6.1.4.1.1466.115.121.1.50 

# LDAP-Attribute		    | defined	    | Field
# -------------------------------------------------------------------------
# uid 0.9.2342.19200300.100.1.1	    | RFC 1274	    | H.323 user
# cn 2.5.4.3			    | RFC 2256(core)| valid H.323-ID
# telephoneNumber 2.5.4.20	    | RFC 2256(core)| valid E164-IDs
# facsimileTelephoneNumber 2.5.4.23 | RFC 2256(core)| for Unified Messaging
# plaintextPassword		    | umuser, mways | H.235 password
# pincode			    | umuser	    | for UMS
#
# voIPnickName			    | HERE	    | valid H.323-ID
# voIPcountryCode		    | HERE	    | see DESC
# voIPareaCode			    | HERE	    | see DESC
# voIPlocalAccessCode		    | HERE	    | see DESC
# voIPnationalAccessCode	    | HERE	    | see DESC
# voIPinternationalAccessCode	    | HERE	    | see DESC
# voIPcallingLineIdPresentation	    | HERE	    | see DESC
# voIPprefixBlacklist		    | HERE	    | see DESC
# voIPprefixWhitelist		    | HERE	    | see DESC
#
# This ObjectClass shall be used to extend objects which typically are
# also of class 'person' or 'inetOrgPerson', 'mways' or 'umuser' or are
# structural nodes like 'organizationalUnit' above of those user
# representating objects.

# A) VoIP User
# 
# A.1) Attributes also used in other mediaWays or std. classes. You may
#      need to remove the comment signs for some of them, if you do not
#      have the 'mways.schema'-file.
# 
# attributetype ( 0.9.2342.19200300.100.1.1 NAME ( 'uid' 'userid' )
#   EQUALITY caseIgnoreMatch
#   SUBSTR caseIgnoreSubstringsMatch
#   SYNTAX 1.3.6.1.4.1.1466.115.121.1.15{256} )
# attributetype ( 2.5.4.3 NAME ( 'cn' 'commonName' )
#   DESC 'common name(s) for which the entity is known by'
#   SUP name )
# attributetype ( 2.5.4.20 NAME 'telephoneNumber'
#   EQUALITY telephoneNumberMatch
#   SUBSTR telephoneNumberSubstringsMatch
#   SYNTAX 1.3.6.1.4.1.1466.115.121.1.50{32} )
# attributetype ( 2.5.4.23 NAME ( 'facsimileTelephoneNumber' 'fax' )
#   SYNTAX 1.3.6.1.4.1.1466.115.121.1.22 )
# 
# attributetype ( 1.3.6.1.4.1.9564.2.1.1.8 NAME 'plaintextPassword'
#   DESC 'password of user -- always cleartext'
#   EQUALITY octetStringMatch
#   SYNTAX 1.3.6.1.4.1.1466.115.121.1.40{128} )
# attributetype	( 1.3.6.1.4.1.9564.2.1.3.2 NAME 'pincode'
#   DESC 'personal identification name'
#   EQUALITY caseExactIA5Match
#   SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
#   SINGLE-VALUE )
# 
# A.2) Own Attributes
#
# NOTE: H.225 defines (in ASN.1):
#  "NumberDigits ::= IA5String (SIZE (1..128)) (FROM ("0123456789#*,"))"
#  That rises the question, if one should map all those types as
#  separate syntax to this specification.
#
# Remark: Because this identifierers are related to the ASN.1 nameing
# rules, all identifierers are starting with a lower case letter,
# seperating words by capital letters.
#


attributeType ( 1.3.6.1.4.1.9564.2.5.1000 NAME 'voIPnickName'
  DESC 'A H.323-Id the user is also known as'
  EQUALITY caseExactIA5Match
  SUBSTR caseExactIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{16}
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2000 
  NAME ( 'voIPcountryCode' 'voIPcc' )
  DESC 'The prefix indicating the user"s country'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2010 
  NAME ( 'voIPareaCode' 'voIPac' )
  DESC 'The prefix indicating the user"s area'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2020 
  NAME ( 'voIPlocalAccessCode' 'voIPlac' )
  DESC 'The prefix code indicating the access to local calls'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2030 
  NAME ('voIPnationalAccessCode' 'voIPnac' )
  DESC 'The prefix code indicating the access to national calls'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2040 
  NAME ( 'voIPinternationalAccessCode' 'voIPiac' )
  DESC 'The prefix code indicating the access to international calls'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2050
  NAME ( 'voIPcallingLineIdPresentation' 'voIPclir' )
  DESC 'Indicates, wether to present the calling line ID to the called
        participant'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2060 NAME 'voIPprefixBlacklist'
  EQUALITY numericStringMatch
  DESC 'A blocked prefix for this user'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
 )

attributeType ( 1.3.6.1.4.1.9564.2.5.2070 NAME 'voIPprefixWhitelist'
  EQUALITY numericStringMatch
  DESC 'An accessible prefix for this user'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{16}
 )

# A.2) Object Classes

objectClass ( 1.3.6.1.4.1.9564.1.5.1000 NAME 'voIPProperty'
  SUP top ABSTRACT
  DESC 'Objects of this class can be used to provide VoIP-Equipment with
        user-related access and authentification information'
  MAY ( voIPnickName $ voIPcountryCode $ voIPareaCode $ voIPlocalAccessCode $
        voIPnationalAccessCode $ voIPinternationalAccessCode $
	voIPcallingLineIdPresentation $ voIPprefixBlacklist $
        voIPprefixWhitelist ))

